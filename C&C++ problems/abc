#include <iostream>
using namespace std;

int main()
{    int n;
     cin>>n;
    int i=1;
    
    while(i<=n){
         int j=1;
          
        while(j<=n){
            //i+j-1=1
            //'A'+i+j-1=1+'A'
            //'A'+i+j-2='A'
           char c='A'+i+j-2;
            cout<<c<<" ";
            j++;
           
        }
        cout<<endl;
        i++;
    }
    return 0;
}
